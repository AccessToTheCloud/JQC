        <td><%= application.job_type %></td>
        <td><%= application.consent %></td>
        <td><%= application.certifier %></td>
        <td><%= application.certification_notes %></td>
        <td><%= application.invoice_to %></td>
        <td><%= application.care_of %></td>
        <td><%= application.invoice_to_id %></td>
        <td><%= application.care_of_id %></td>
        <td><%= application.invoice_email %></td>
        <td><%= application.attention %></td>
        <td><%= application.purchase_order_number %></td>
        <td><%= application.fully_invoiced %></td>
        <td><%= application.invoice_debtor_notes %></td>
        <td><%= application.applicant_email %></td>
        <td><%= application.sort_priority_gen %></td>
        <td><%= link_to 'Show', application %></td>
        <td><%= link_to 'Edit', edit_application_path(application) %></td>
        <td><%= link_to 'Destroy', application, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
